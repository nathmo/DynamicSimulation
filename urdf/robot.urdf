<robot name="robot">
  <link name="base_footprint"></link>
  <joint name="base_joint" type="fixed">
    <parent link="base_footprint" />
    <child link="base_link" />
    <origin xyz="0 0 1" rpy="0 0 0" />
  </joint>
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="2.5 0.75 0.1" />
      </geometry>
      <material name="base_link-material">
        <color rgba="0.05 0.05 0.05 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><box size="2.5 0.75 0.1"/></geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="100" />
      <inertia ixx="10" ixy="0" ixz="0" iyy="10" iyz="0" izz="10" />
    </inertial>
  </link>

  <joint name="base_link_to_hipFL" type="revolute">
    <parent link="base_link" />
    <child link="hipFL" />
    <origin xyz="1 0.45 0" rpy="0 0 0" />
    <axis xyz="0 -1 0"/>
    <limit effort="1000.0" lower="-1.5" upper="0" velocity="0.5"/>
  </joint>
  <link name="hipFL">
    <visual>
      <origin xyz="0.5 0 0" rpy="0 0 0" />
      <geometry>
        <box size="1 0.1 0.1" />
      </geometry>
      <material name="hipFL-material">
        <color rgba="0.1 0.3 0.7 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0.5 0 0" rpy="0 0 0" />
      <geometry>
        <box size="1 0.1 0.1" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.5 0 0" rpy="0 0 0" />
      <mass value="5" />
      <!-- inertia scaled from 0.1 to 0.5 for a 5 kg mass -->
      <inertia ixx="0.5" ixy="0" ixz="0" iyy="0.5" iyz="0" izz="0.5" />
    </inertial>
  </link>

